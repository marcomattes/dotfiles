{
  "env":{
     "browser":true,
     "node":true,
     "amd":true,
     "jquery":true
  },
  "globals":{
     "angular":false,
     "Snap":true,
     "mina":false
  },
  "parserOptions":{
     "ecmaVersion":6,
     "sourceType":"module",
     "ecmaFeatures":{
        "jsx":true
     }
  },
  "rules":{
     "no-cond-assign":2,
     "no-console":0,
     "no-constant-condition":2,
     "no-control-regex":2,
     "no-debugger":1,
     "no-dupe-args":2,
     "no-dupe-keys":2,
     "no-duplicate-case":2,
     "no-empty-character-class":0,
     "no-empty":1,
     "no-ex-assign":2,
     "no-extra-boolean-cast":2,
     "no-extra-parens":2,
     "no-extra-semi":2,
     "no-func-assign":2,
     "no-inner-declarations":[
        2,
        "functions"
     ],
     "no-invalid-regexp":2,
     "no-irregular-whitespace":2,
     "no-negated-in-lhs":2,
     "no-obj-calls":2,
     "no-regex-spaces":2,
     "no-sparse-arrays":2,
     "no-unreachable":2,
     "use-isnan":2,
     "valid-jsdoc":2,
     "valid-typeof":2,
     "no-unexpected-multiline":0,
     "accessor-pairs":0,
     "block-scoped-var":1,
     "complexity":0,
     "consistent-return":1,
     "curly":[
        2,
        "all"
     ],
     "default-case":1,
     "dot-notation":0,
     "dot-location":[
        1,
        "property"
     ],
     "eqeqeq":2,
     "guard-for-in":2,
     "no-alert":2,
     "no-caller":1,
     "no-div-regex":0,
     "no-else-return":2,
     "no-empty-label":0,
     "no-eq-null":2,
     "no-eval":2,
     "no-extend-native":2,
     "no-extra-bind":2,
     "no-fallthrough":1,
     "no-floating-decimal":0,
     "no-implicit-coercion":0,
     "no-implied-eval":2,
     "no-invalid-this":2,
     "no-iterator":0,
     "no-labels":2,
     "no-lone-blocks":2,
     "no-loop-func":2,
     "no-multi-spaces":2,
     "no-multi-str":2,
     "no-native-reassign":2,
     "no-new":0,
     "no-new-func":0,
     "no-new-wrappers":2,
     "no-octal":2,
     "no-octal-escape":2,
     "no-param-reassign":0,
     "no-process-env":0,
     "no-proto":1,
     "no-redeclare":1,
     "no-return-assign":0,
     "no-script-url":0,
     "no-self-compare":2,
     "no-sequences":2,
     "no-throw-literal":2,
     "no-unused-expressions":2,
     "no-useless-call":2,
     "no-useless-concat":1,
     "no-void":2,
     "no-warning-comments":0,
     "no-with":2,
     "radix":1,
     "vars-on-top":0,
     "wrap-iife":[
        2,
        "any"
     ],
     "yoda":0,
     "strict":0,
     "init-declarations":0,
     "no-catch-shadow":0,
     "no-delete-var":1,
     "no-label-var":0,
     "no-shadow-restricted-names":2,
     "no-shadow":1,
     "no-undef-init":0,
     "no-undef":2,
     "no-undefined":0,
     "no-unused-vars":[
        1,
        {
           "vars":"local",
           "args":"after-used"
        }
     ],
     "no-use-before-define":0,
     "array-bracket-spacing":[
        2,
        "never"
     ],
     "block-spacing":[
        2,
        "always"
     ],
     "brace-style":[
        2,
        "1tbs",
        {
           "allowSingleLine":true
        }
     ],
     "camelcase":1,
     "comma-spacing":[
        2,
        {
           "before":false,
           "after":true
        }
     ],
     "comma-style":[
        2,
        "last"
     ],
     "computed-property-spacing":[
        2,
        "never"
     ],
     "consistent-this":0,
     "eol-last":0,
     "func-names":0,
     "func-style":0,
     "id-length":0,
     "indent":[
        2,
        2,
        {
           "SwitchCase":1
        }
     ],
     "jsx-quotes":0,
     "key-spacing":[
        2,
        {
           "beforeColon":false,
           "afterColon":true
        }
     ],
     "lines-around-comment":0,
     "linebreak-style":0,
     "max-nested-callbacks":[
        1,
        3
     ],
     "new-cap":2,
     "new-parens":2,
     "newline-after-var":0,
     "no-array-constructor":2,
     "no-continue":0,
     "no-inline-comments":0,
     "no-lonely-if":0,
     "no-mixed-spaces-and-tabs":2,
     "no-multiple-empty-lines":[
        1,
        {
           "max":5
        }
     ],
     "no-nested-ternary":1,
     "no-new-object":2,
     "no-restricted-syntax":0,
     "no-spaced-func":2,
     "no-ternary":0,
     "no-trailing-spaces":2,
     "no-underscore-dangle":0,
     "no-unneeded-ternary":0,
     "object-curly-spacing":[
        2,
        "never"
     ],
     "one-var":0,
     "operator-assignment":0,
     "operator-linebreak":[
        2,
        "after"
     ],
     "padded-blocks":0,
     "quote-props":0,
     "quotes":[
        2,
        "single"
     ],
     "id-match":0,
     "semi-spacing":[
        2,
        {
           "before":false,
           "after":true
        }
     ],
     "semi":2,
     "sort-vars":0,
     "keyword-spacing":[
        2,
        {
           "before":true,
           "after":true,
           "overrides":{

           }
        }
     ],
     "space-before-blocks":[
        2,
        "always"
     ],
     "space-before-function-paren":[
        0,
        "always"
     ],
     "space-in-parens":[
        2,
        "never"
     ],
     "space-infix-ops":1,
     "space-unary-ops":[
        2,
        {
           "words":true,
           "nonwords":false
        }
     ],
     "spaced-comment":0,
     "wrap-regex":0
  }
}
